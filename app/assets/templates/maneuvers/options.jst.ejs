<div class="prompt">
  <div class="padded">
    <%=prompt%>
    <div class="button-group">
      <% _.each(options, function(option) { %>
        <button type="button" class="submit-button"><%=option%></button>
      <% }) %>
    </div>
  </div>
</div>

<div class="asset">
  <div class="padded">
    <img src="<%=image_url%>" />
  </div>
</div>

<a href="<%=credit_url%>" class="credit-link" target="_blank"><%=credit%></a>
